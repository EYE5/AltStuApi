extends layout

block content
  h1 Welcome to AltStu Open API
  h2 Endpoints
  ul
    li 
      h2 POST
      h2 /login
      h3 Params
      h4 login - string
      h4 password - string
      h3 Returns
      h4 sessionid - string
    li
      h2 GET
      h2 /schedule
      h3 Params
      h4 sessionid - string
      h3 Returns
      h4 schedules - Array
    li
      h2 GET
      h2 /messages/unread
      h3 Params
      h4 sessionid - string
      h3 Returns
      h4 messages - Array
    li
      h2 GET
      h2 /messages/archive
      h3 Params
      h4 sessionid - string
      h3 Returns
      h4 messages - Array
    li
      h2 GET
      h2 /files
      h3 Params
      h4 sessionid - string
      h3 Returns
      h4 files - Array    

